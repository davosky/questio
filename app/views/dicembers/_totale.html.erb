      <div class="card-header">
        <i class="fa-regular fa-calendar"></i>
        &nbsp;
        Anno:&emsp;
        <span class="fw-bold">
          <%= previous_year %>
        </span>
      </div>
      <div class="card-body">
        <i class="fa-solid fa-users"></i>
        &nbsp;
