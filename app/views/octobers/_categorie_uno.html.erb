          <i class="fa-solid fa-users"></i>
          &nbsp;
          <%= "#{@oct_curs_filcams.count}".html_safe %>
        </span>
        <br/>
        <i class="fa-solid fa-arrows-left-right-to-line"></i>
        &nbsp;
