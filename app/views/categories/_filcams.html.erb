    <br>
    &emsp;
    <%= line_chart({"gen" => "#{jan_trend_filcams}", "feb" => "#{feb_trend_filcams}", "mar" => "#{mar_trend_filcams}", "apr" => "#{apr_trend_filcams}", "mag" => "#{may_trend_filcams}", "giu" => "#{jun_trend_filcams}", "lug" => "#{jul_trend_filcams}", "ago" => "#{aug_trend_filcams}", "set" => "#{sep_trend_filcams}", "ott" => "#{oct_trend_filcams}", "nov" => "#{nov_trend_filcams}", "dic" => "#{dic_trend_filcams}"}, height: "120px",colors: ["#{filcams_trend_resulting_color}"]) %>
